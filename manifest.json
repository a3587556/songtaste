{  
  "name": "SongTaste", 
  "version": "0.1.0",
  "manifest_version": 2,  
  "description": "display song's URL", 
  "background": { "scripts": ["background.js"] },
  "permissions": [
	"tabs",
	"http://*/",
	"storage" 
	],
  "browser_action": {  
    "default_icon": "icon.png" ,
    "default_title": "SongTaste",
    "default_popup": "popup.html"
  }
} 